<div id="<%= dom_id event %>">
  <p>
    <strong>Event name:</strong>
    <%= event.event_name %>
  </p>

  <p>
    <strong>Event type:</strong>
    <%= event.event_type %>
  </p>

  <p>
    <strong>Event url:</strong>
    <%= event.event_url %>
  </p>

  <p>
    <strong>Zoom code:</strong>
    <%= event.zoom_code %>
  </p>

  <p>
    <strong>Rsvp needed:</strong>
    <%= event.rsvp_needed %>
  </p>

  <p>
    <strong>Maximum capacity:</strong>
    <%= event.maximum_capacity %>
  </p>

  <p>
    <strong>Start time:</strong>
    <%= event.start_time %>
  </p>

  <p>
    <strong>End time:</strong>
    <%= event.end_time %>
  </p>

  <p>
    <strong>Members only:</strong>
    <%= event.members_only %>
  </p>

  <p>
    <strong>Lounge:</strong>
    <%= event.lounge_id %>
  </p>

</div>
